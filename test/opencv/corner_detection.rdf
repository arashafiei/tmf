rdf corner_detection {
    topology {
        nodes = A,B,C,D,E;
	edges = e1(A,B), e2(B,C), e3(C,D), e4(D,E), e5(A,C), e6(A,D);
    }
    actor A {
    	computation = VideoCapture;
        file_name = pedestrian.mp4;
    }
    actor B {
    	computation = Dilate;
        dilation_size = 2;
    }
    actor C {
    	computation = Compare;
        operation = eq;
    }
    actor D {
    	computation = Multiply;
    }
    actor E {
    	computation = ImageWrite;
    }
}

