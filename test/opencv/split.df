df imwrite {
    topology {
        nodes = A,B,C,D;
	edges = e1(A,B), e2(B,C), e3(C,D);
    }
    actor A {
    	computation = VideoCapture;
	file_name = pedestrian.mp4;
	host = 127.0.0.1;
    }
    actor B {
    	computation = MatSplit;
	level = 1;
	host = 127.0.0.1;
    }
    actor C {
    	computation = MatMerge;
	level = 1;
	host = 127.0.0.1;
    }
    actor D {
    	computation = ImageWrite;
	host = 127.0.0.1;
    }
    parameter {
	distributed = false;
	discovery_host = 127.0.0.1;
	discovery_port = 7000;
    }
}
