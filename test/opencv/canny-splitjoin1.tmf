tmf canny-splitjoin1 {
    topology {
        nodes = A,AS,B,C,DJ,D;
	edges = e0(A,AS),e1(AS,B), e2(B,C), e3(C,DJ), e4(DJ,D);
    }
    actor A {
    	computation = VideoCaptureMR;
	file_name = pedestrian.mp4;
    }
    actor AS {
    	computation = MatSplit;
	level = 1;
    }
    actor B {
    	computation = CvtColor;
    }
    actor C {
    	computation = Canny;
    }
    actor DJ {
    	computation = MatJoin;
	level = 1;
    }
    actor D {
    	computation = ImageWriteMR;
    }
    parameter {
	logging = true;
    }
}
