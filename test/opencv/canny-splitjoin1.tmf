tmf canny-splitjoin1 {
    topology {
        nodes = A,B,C,D,E,F;
	edges = e01(A,B), e02(B,C), e03(C,D), e04(D,E), e05(E,F);
    }
    actor A {
    	computation = VideoCapture;
	file_name = pedestrian.mp4;
    }
    actor B {
    	computation = MatSplit;
	level = 1;
    }
    actor C {
    	computation = CvtColor;
    }
    actor D {
    	computation = Canny;
    }
    actor E {
    	computation = MatJoin;
	level = 1;
    }
    actor F {
    	computation = ImageShow;
    }
    parameter {
	logging = true;
    }
}
