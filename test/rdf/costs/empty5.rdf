rdf dummy5 {
    topology {
        nodes = Src, A1, A2, A3, A4, A5, Snk;
	edges = e1(Src,A1), e2(A1,A2), e3(A2,A3), e4(A3,A4),
		e5(A4,A5), e6(A5,Snk);
    }
    actor Src { computation = DelaySource; last=100; delay = 10; }

    actor A1 { computation = DelayActor; delay = 10; }
    actor A2 { computation = DelayActor; delay = 10; }
    actor A3 { computation = DelayActor; delay = 10; }
    actor A4 { computation = DelayActor; delay = 10; }
    actor A5 { computation = DelayActor; delay = 10; }
    actor Snk { computation = DelaySink; delay = 10; }
    parameter {
	logging = false;
	scheduling = false;
    }
}
rule tr1 {
left {
    topology {
        nodes = Src, A1;
	edges = e1(Src,A1);
    }
}
right {
    topology {
        nodes = Src, A1;
	edges = e1(Src,A1);
    }
}
}
main {
   Src.timer = 1000 : tr1;
}
