rdf dummy-b1 {
    topology {
        nodes = Src, A, Snk;
	edges = e1(Src,A), e2(A,Snk);
    }
    actor Src { computation = DelaySource; last=100; delay = 10; output_size=6; }

    actor A { computation = DelayActor; delay = 10; output_size=6; }
    actor Snk { computation = DelaySink; delay = 10; }
    parameter {
	logging = false;
	scheduling = false;
    }
}
rule tr1 {
left {
    topology {
        nodes = Src, A, Snk;
	edges = e1(Src,A), e2(A,Snk);
    }
}
right {
    topology {
        nodes = Src, A, Snk;
	edges = e1(Src,A), e2(A,Snk);
    }
}
}
main {
   Src.timer = 1000 : tr1;
}
