rdf inc1-b8 {
    topology {
        nodes = Src, Snk;
	edges = e1(Src,Snk);
    }
    actor Src { computation = DelaySource; last=100; delay = 10; output_size=8; }
    actor Snk { computation = DelaySink; delay = 20; }
    parameter {
	logging = false;
	scheduling = false;
    }
}
rule tr1 {
left {
    topology {
        nodes = Src, Snk;
	edges = e1(Src,Snk);
    }
}
right {
    topology {
        nodes = Src, x, Snk;
	edges = e1(Src,x), e2(x,Snk);
    }
    actor x { computation = DelayActor; delay = 10; }
}
}
main {
   Src.timer = 1000 : tr1;
}
