rdf empty-b5 {
    topology {
        nodes = Src, A, Snk;
	edges = e1(Src,A), e2(A,Snk);
    }
    actor Src { computation = DelaySource; last=100; delay = 10; output_size=5; }

    actor A { computation = DelayActor; delay = 30; output_size=5; }
    actor Snk { computation = DelaySink; delay = 10; }
    parameter {
	logging = false;
	scheduling = false;
    }
}
rule tr1 {
left {
    topology {
        nodes = Src, A, Snk;
	edges = e1(Src,A), e2(A,Snk);
    }
}
right {
    topology {
        nodes = Src, A, Snk;
	edges = e1(Src,A), e2(A,Snk);
    }
}
}
main {
}
