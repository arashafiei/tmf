rdf exp3 {
    topology {
        nodes = Src,A1,B1,C1,D1,Snk;
	edges = e1(Src,A1), e2(A1,B1), e3(B1,C1),
		e4(C1,D1), e5(A1,D1), e6(D1,Snk);
    }
    actor Src {
    	computation = DelaySource;
	delay = 10;
	last = 500;
    }
    actor A1 {
    	computation = DelayGeneric;
	delay = 50;
    }
    actor B1 {
    	computation = DelayGeneric;
	delay = 10;
    }
    actor C1 {
    	computation = DelayGeneric;
	delay = 10;
    }
    actor D1 {
    	computation = DelayGeneric;
	delay = 50;
    }
    actor Snk {
    	computation = DelaySink;
	delay = 10;
    }
    parameter {
	logging = false;
	scheduling = false;
	policy = 0;
    }
}
rule r1 {
left {
    topology {
        nodes = A1,B1,C1,D1;
	edges = e1(A1,B1), e2(B1,C1),
		e3(C1,D1), e4(A1,D1);
    }
    actor A1 {
    	computation = DelayGeneric;
    }
    actor B1 {
    	computation = DelayGeneric;
    }
    actor C1 {
    	computation = DelayGeneric;
    }
    actor D1 {
    	computation = DelayGeneric;
    }
}
right {
    topology {
        nodes = A1,B1,C1,D1;
	edges = e1(A1,B1), e2(B1,D1), 
		e3(C1,D1), e4(A1,C1);
    }
    actor A1 {
    	computation = DelayGeneric;
    }
    actor B1 {
    	computation = DelayGeneric;
    }
    actor C1 {
    	computation = DelayGeneric;
    }
    actor D1 {
    	computation = DelayGeneric;
    }
}
}
main {
   Src.timer = 10000 : r1;
}
