rdf exp1 {
    topology {
        nodes = Src,A1,B1,Snk;
	edges = e1(Src,A1), e2(A1,B1), e3(B1,Snk);
    }
    actor Src {
    	computation = DelaySource;
	delay = 10;
	last = 1000;
    }
    actor A1 {
    	computation = DelayActorA;
	delay = 40;
    }
    actor B1 {
    	computation = DelayActorB;
	delay = 20;
    }
    actor Snk {
    	computation = DelaySink;
	delay = 10;
    }
    parameter {
	logging = false;
	scheduling = false;
	policy = 0;
    }
}
rule r1 {
left {
    topology {
        nodes = ax,B1;
	edges = e1(ax,B1);
    }
    actor ax {
    	computation = DelayActorA;
    }
    actor B1 {
    	computation = DelayActorB;
    }
}
right {
    topology {
        nodes = ax,ay,B1;
	edges = e1(ax,ay), e2(ay,B1);
    }
    actor ax {
    	computation = DelayActorA;
    }
    actor ay {
    	computation = DelayActorA;
	delay = 40;
    }
    actor B1 {
    	computation = DelayActorB;
    }
}
}
rule r2 {
left {
    topology {
        nodes = ax,ay,B1;
	edges = e1(ax,ay), e2(ay,B1);
    }
    actor ax {
    	computation = DelayActorA;
    }
    actor ay {
    	computation = DelayActorA;
    }
    actor B1 {
    	computation = DelayActorB;
    }
}
right {
    topology {
        nodes = ax,B1;
	edges = e1(ax,B1);
    }
    actor ax {
    	computation = DelayActorA;
    }
    actor B1 {
    	computation = DelayActorB;
    }
}
}
main {
   Src.timer = 5000 : r1;
   Src.timer = 10000 : r1;
   Src.timer = 20000 : r2;
   Src.timer = 25000 : r2;
}
