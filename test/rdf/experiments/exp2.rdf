rdf exp2 {
    topology {
        nodes = Src,S,A1,B1,J,Snk;
	edges = e1(Src,S), e2(S,A1), e3(A1,B1),
		e4(B1,J), e5(J,Snk);
    }
    actor Src {
    	computation = DelaySource;
	delay = 10;
	last = 1200;
    }
    actor S {
	computation = DelaySplit;
	arity = 1;
	delay = 2;
    }
    actor A1 {
    	computation = DelayActorA;
	delay = 40;
    }
    actor B1 {
    	computation = DelayActorB;
	delay = 40;
    }
    actor J {
	computation = DelayJoin;
	arity = 1;
	delay = 2;
    }
    actor Snk {
    	computation = DelaySink;
	delay = 10;
    }
    parameter {
	logging = false;
	scheduling = false;
	policy = 0;
    }
}
rule tr1 {
left {
    topology {
        nodes = S,A1,B1,J;
	edges = e1(S,A1), e2(A1,B1), e3(B1,J);
    }
    actor S {
    	computation = DelaySplit;
	arity = 1;
    }
    actor A1 {
    	computation = DelayActorA;
    }
    actor B1 {
    	computation = DelayActorB;
    }
    actor J {
    	computation = DelayJoin;
	arity = 1;
    }
}
right {
    topology {
        nodes = S,A1,B1,ax,bx,J;
	edges = e1(S,A1), e2(S,ax), e3(A1,B1), 
		e4(ax,bx), e5(B1,J), e6(bx,J);
    }
    actor S {
    	computation = DelaySplit;
	arity = 2;
    }
    actor A1 {
    	computation = DelayActorA;
    }
    actor B1 {
    	computation = DelayActorB;
    }
    actor ax {
    	computation = DelayActorA;
	delay = 40;
    }
    actor bx {
    	computation = DelayActorB;
	delay = 40;
    }
    actor J {
    	computation = DelayJoin;
	arity = 2;
    }
}
}
rule tr2 {
left {
    topology {
        nodes = S,A1,B1,ax,bx,J;
	edges = e1(S,A1), e2(S,ax), e3(A1,B1), 
		e4(ax,bx), e5(B1,J), e6(bx,J);
    }
    actor S {
    	computation = DelaySplit;
	arity = 2;
    }
    actor A1 {
    	computation = DelayActorA;
    }
    actor B1 {
    	computation = DelayActorB;
    }
    actor ax {
    	computation = DelayActorA;
    }
    actor bx {
    	computation = DelayActorB;
    }
    actor J {
    	computation = DelayJoin;
	arity = 2;
    }
}
right {
    topology {
        nodes = S,A1,B1,J;
	edges = e1(S,A1), e2(A1,B1), e3(B1,J);
    }
    actor S {
    	computation = DelaySplit;
	arity = 1;
    }
    actor A1 {
    	computation = DelayActorA;
    }
    actor B1 {
    	computation = DelayActorB;
    }
    actor J {
    	computation = DelayJoin;
	arity = 1;
    }
}
}
main {
   Src.timer = 5000 : tr1;
   Src.timer = 10000 : tr1;
   Src.timer = 20000 : tr2;
   Src.timer = 25000 : tr2;
}
