rdf demo {
    topology {
        nodes = A,S,B,C,D,J,I;
	edges = e1(A,S), e2(S,B), e3(B,C), e4(C,D), e5(D,J), e6(J,I);
    }
    actor A {
    	computation = VideoCaptureMR;
	file_name = pedestrian.mp4;
	period = 10;
    }
    actor S {
	computation = MatSplit;
	arity = 1;
    }
    actor B {
    	computation = CvtColor;
    }
    actor C {
    	computation = Canny;
    }
    actor D {
	computation = DummyLoad;
    }
    actor J {
	computation = MatJoin;
	arity = 1;
    }
    actor I {
    	computation = ImageShowMR;
	period = 10;
    }
    parameter {
	logging = true;
    }
}
rule r1 {
left {
    topology {
        nodes = S,x,y,z,J;
	edges = e1(S,x), e2(x,y), 
		e3(y,z), e4(z,J);
    }
    actor S {
    	computation = MatSplit;
	arity = 1;
    }
    actor x {
    	computation = CvtColor;
    }
    actor y {
    	computation = Canny;
    }
    actor z {
	computation = DummyLoad;
    }
    actor J {
    	computation = MatJoin;
	arity = 1;
    }
}
right {
    topology {
        nodes = S,x,x1,y,y1,z,z1,J;
	edges = e1(S,x), e2(S,x1), e3(x,y), 
		e4(x1,y1), e5(y,z), e6(y1,z1), 
		e7(z,J), e8(z1,J);
    }
    actor S {
    	computation = MatSplit;
	arity = 2;
    }
    actor x {
    	computation = CvtColor;
    }
    actor x1 {
    	computation = CvtColor;
    }
    actor y {
    	computation = Canny;
    }
    actor y1 {
    	computation = Canny;
    }
    actor z {
	computation = DummyLoad;
    }
    actor z1 {
	computation = DummyLoad;
    }
    actor J {
    	computation = MatJoin;
	arity = 2;
    }
}
}
main {
   I.period = 50 : r1;
}
