rdf canny {
    topology {
        nodes = A,D;
	edges = e1(A,D);
    }
    actor A {
    	computation = VideoCapture;
        file_name = pedestrian.mp4;
    }
    actor D {
    	computation = ImageWrite;
    }
}
rule r1 {
left {
    topology {
        nodes = A,x;
	edges = e1(A,x);
    }

    actor A {
    	computation = VideoCapture;
        file_name = pedestrian.mp4;
    }
    actor x {
    	computation = ImageWrite;
    }
}
right {
    topology {
        nodes = A,b,c,x;
	edges = e2(A,b), e3(b,c), e4(c,x);
    }
    actor A {
    	computation = VideoCapture;
        file_name = pedestrian.mp4;
    }
    actor b {
    	computation = CvtColor;
    }
    actor c {
    	computation = Canny;
	threshold = 100;
	ratio = 2;
    }
    actor x {
    	computation = ImageWrite;
    }
}
}
main {
    load=10 : r1;
}
