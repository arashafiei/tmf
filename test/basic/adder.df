df adder {
    topology {
        nodes = A,B,C;
        edges = e1(A,C), e2(B,C);
    }
    actor A {
        computation = IntProducer;
        last = 64;
    }
    actor B {
        computation = IntProducer;
        last = 64;
    }
    actor C {
        computation = Addition;
	input_1_port = 7000;
	input_2_port = 7001;
    }
    parameter {
	realtime = false;
	distributed = false;
    }
}
